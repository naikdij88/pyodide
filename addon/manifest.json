{
  "description": "Add a python console to the developer tools.",
  "manifest_version": 2,
  "name": "PyConsole",
  "version": "1.0",
  "author": "Sarah Bird",
  "homepage_url": "https://github.com/birdsarah/pyodide/tree/pyconsole",
  "icons": {
    "128": "icons/pyconsole.png"
  },
  
  "background": {
    "scripts": ["src/background.js"]
  },
  
  "permissions": [
      "<all_urls>"
  ],
  
  "devtools_page": "src/devtools.html"

}
